{
  "compilerOptions": {
      "target": "esnext",
      "module": "commonjs",
      "outDir": "./dist_bin",
      "strict": true,
      "esModuleInterop": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "allowSyntheticDefaultImports": true,
      "sourceMap": false,
      "noImplicitReturns": true,
      "noImplicitAny": true,
      "allowJs": true,
      "resolveJsonModule": true,
      "downlevelIteration": true
  },
  "include": [
      "bin/**/*"
  ]
}